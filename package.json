    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "firebase": "^4.11.0",
    "firebase-admin": "^5.10.0",
    "firebase-functions": "^0.8.1",
    "flexbox-react": "^4.4.0",
    "fs-extra": "3.0.1",
    "geolib": "^2.0.24",
    "google-distance-matrix": "^1.1.1",
    "google-maps-react": "^1.1.4",
